<h1>商品一覧</h1>

<table>
  <tr>
    <th>商品ID</th>
    <th>商品名</th>
    <th>税抜価格</th>
    <th>ジャンル</th>
    <th>ステータス</th>
  </tr>
  <% @items.each do |item| %>
  <tr>
    <td><%= item.id %></td>
    <td>
      <% link_to admin_items_path(@item.id) do %>
        <%= item.name %>
      <% end %>
    </td>
    <td><%= item.price %></td>
    <td><%= item.genre %></td>
    <td></td>
  </tr>
  <% end %>
</table>